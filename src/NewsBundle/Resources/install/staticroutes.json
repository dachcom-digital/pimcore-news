{
    "routes": [
        {
            "name": "news_detail",
            "pattern": "/(\\b(?!admin\\b)[a-zA-Z0-9-_]+)\\/news\\/(.*?)$/",
            "reverse": "/%_locale/news/%entry",
            "controller": "NewsBundle\\Controller\\EntryController::detailAction",
            "variables": "_locale,entry",
            "priority": "0"
        }
    ]
}